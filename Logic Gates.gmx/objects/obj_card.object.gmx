if (q_size &gt; 1){
    card1=ds_list_find_value(obj_queue.queue, q_size-2);    
    card2=ds_list_find_value(obj_queue.queue, q_size-1);    
    
    if selected {
        op = instance_create((card1.x + card2.x)/2, card1.y, obj_operator);
        ds_list_add(obj_queue.operators, op);
    }
}

if not selected and (q_size &gt;= 1){
    op_pos=max(0,q_pos-1)
    op= ds_list_find_value(obj_queue.operators, op_pos);
    
    if(ds_list_size(obj_queue.queue)!=2){
        ds_list_delete(obj_queue.operators, op_pos);
        with(op){instance_destroy();}
    }
}
